{
  "name": "ipv6",
  "description": "A library for parsing IPv6 (and IPv4) addresses in node and the browser.",
  "keywords": [
    "ipv6",
    "browser",
    "validation"
  ],
  "version": "3.1.1",
  "author": "Beau Gunderson <beau@beaugunderson.com> (http://beaugunderson.com/)",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec",
    "posttest": "./coverage.js"
  },
  "bin": {
    "ipv6": "bin/ipv6.js",
    "ipv6grep": "bin/ipv6grep.js"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/beaugunderson/javascript-ipv6.git"
  },
  "dependencies": {
    "cli": "0.6.x",
    "cliff": "0.1.x",
    "coveralls": "^2.11.2",
    "jsbn": "0.0.0",
    "lodash": "^2.4.1",
    "mocha-lcov-reporter": "0.0.1",
    "sprintf": "0.1.x"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "1.10.x",
    "mocha": "2.0.x"
  },
  "config": {
    "blanket": {
      "pattern": [
        "ipv4.js",
        "ipv6.js",
        "common.js",
        "attributes.js",
        "constants.js",
        "html.js",
        "regular-expressions.js"
      ]
    }
  }
}
