{
  "name": "ipv6",
  "description": "A library for parsing IPv6 (and IPv4) addresses in node and the browser.",
  "keywords": [
    "ipv6",
    "browser",
    "validation"
  ],
  "version": "3.1.1",
  "author": "Beau Gunderson <beau@beaugunderson.com> (http://beaugunderson.com/)",
  "license": "MIT",
  "main": "ipv6",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec",
    "posttest": "./node_modules/.bin/mocha --require blanket -R html-cov > coverage.html"
  },
  "bin": {
    "ipv6": "bin/ipv6.js",
    "ipv6grep": "bin/ipv6grep.js"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/beaugunderson/javascript-ipv6.git"
  },
  "dependencies": {
    "sprintf": "0.1.x",
    "cli": "0.4.x",
    "cliff": "0.1.x"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "1.8.x",
    "mocha": "1.15.x"
  },
  "config": {
    "blanket": {
      "pattern": "ipv6.js"
    }
  }
}
